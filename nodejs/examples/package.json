    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-node-single-context": "^29.4.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
